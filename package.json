{
  "name": "boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm exec vite",
    "build": "pnpm exec tsc -b && pnpm exec vite build",
    "lint": "pnpm exec eslint .",
    "format": "pnpm exec prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "pnpm exec jest --passWithNoTests",
    "preview": "pnpm exec vite preview",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "type-check": "pnpm exec tsc --noEmit",
    "type-check:lint": "pnpm exec tsc --noEmit --skipLibCheck -p tsconfig.lint.json"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "pnpm exec prettier --write",
      "pnpm exec eslint --fix",
      "pnpm exec eslint",
      "./scripts/type-check-lint.sh",
      "pnpm test --findRelatedTests"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.2",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@reduxjs/toolkit": "^2.9.0",
    "antd": "^5.27.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-redux": "^9.1.0",
    "react-router-dom": "^7.9.1",
    "redux": "^5.0.1",
    "redux-saga": "^1.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@faker-js/faker": "^10.0.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "eslint": "^9.35.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "lint-staged": "^16.1.6",
    "miragejs": "^0.1.48",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.2",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.5",
    "vite-plugin-checker": "^0.10.3"
  }
}
